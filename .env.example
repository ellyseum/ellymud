MAX_PASSWORD_ATTEMPTS=3

# MCP Server API Key
# Generate with: openssl rand -hex 32
# Or leave blank and the server will prompt to generate one automatically on first start
# Required for MCP server authentication (GitHub Copilot, AI agents)
ELLYMUD_MCP_API_KEY=your_generated_api_key_here
# MUD Admin JWT Secret
# Set this to a secure random string (e.g., openssl rand -base64 32)
# Required for admin authentication
JWT_SECRET=mud-admin-secret-key